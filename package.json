{
	"name": "@skyra/teryl",
	"version": "1.0.0",
	"description": "Your favourite Discord bot for tools, part of ArchId Network",
	"author": "Skyra Project <contact@skyra.pw>",
	"license": "Apache-2.0",
	"private": true,
	"main": "dist/main.js",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#generated/*": "./src/generated/*"
	},
	"scripts": {
		"build": "tsc -b src",
		"dev": "yarn build && yarn start",
		"watch": "tsc -b src -w",
		"prisma:generate": "yarn prisma generate",
		"clean": "rimraf dist/",
		"start": "node --enable-source-maps dist/main.js",
		"test": "vitest run",
		"lint": "eslint --fix --ext ts src tests",
		"format": "prettier --write --loglevel=warn \"{src,tests,scripts}/**/*.{js,ts,json}\"",
		"update": "yarn upgrade-interactive",
		"postinstall": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/builders": "^1.6.1",
		"@discordjs/collection": "^1.5.0",
		"@napi-rs/canvas": "^0.1.40",
		"@prisma/client": "^4.13.0",
		"@sapphire/async-queue": "^1.5.0",
		"@sapphire/duration": "^1.1.0",
		"@sapphire/result": "^2.6.2",
		"@sapphire/utilities": "^3.11.1",
		"@skyra/env-utilities": "^1.2.0",
		"@skyra/http-framework": "^1.0.1",
		"@skyra/http-framework-i18n": "^1.0.0",
		"@skyra/logger": "^2.0.1",
		"@skyra/safe-fetch": "^1.1.1",
		"@skyra/shared-http-pieces": "^1.0.1",
		"@skyra/start-banner": "^2.0.0",
		"@skyra/twitch-helpers": "^1.0.2",
		"@vladfrangu/async_event_emitter": "^2.2.1",
		"canvas-constructor": "^7.0.0",
		"discord-api-types": "^0.37.39",
		"gradient-string": "^2.0.2",
		"he": "^1.2.0",
		"ioredis": "^5.3.2",
		"jsbd": "^0.0.11",
		"luxon": "^3.3.0",
		"nanoid": "^4.0.2",
		"tslib": "^2.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@sapphire/eslint-config": "^4.4.1",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.0",
		"@types/gradient-string": "^1.1.2",
		"@types/he": "^1.2.0",
		"@types/luxon": "^3.3.0",
		"@types/node": "^18.16.3",
		"@types/tar": "^6.1.4",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"@vitest/coverage-c8": "^0.30.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"prisma": "^4.13.0",
		"rimraf": "^5.0.0",
		"tar": "^6.1.13",
		"typescript": "^5.0.4",
		"vitest": "^0.30.1"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire",
		"rules": {
			"@typescript-eslint/no-base-to-string": "off"
		}
	},
	"packageManager": "yarn@3.5.0",
	"volta": {
		"node": "18.16.0"
	}
}
